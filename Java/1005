import java.util.Locale;
import java.util.Scanner; 
 
public class Main {
 
        public static void main(String[] args){
        
        Locale.setDefault(Locale.US);
        
        Scanner sc = new Scanner(System.in);        
        double A, B, med;
     
        A = sc.nextDouble();
        B = sc.nextDouble();
        
        med = ((A*3.5) + (B*7.5)) / 11.0;
        
        System.out.printf("MEDIA = %.5f\n",med);
        
        sc.close();
    
    } 
}
